<div class="touch-radio {{#if checked}}checked{{/if}}" data-value="{{position}}">
  {{text}}
  {{#if hint}}
  	<span class="radio-choice-hint">{{hint}}</span>
  {{/if}}
</div>
{{#if specify}}
<input class="form-control specify-input" data-id="{{id}}" type="text" id="specify_{{id}}" value="{{specifyValue}}">
{{/if}}